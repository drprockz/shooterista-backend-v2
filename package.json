  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "prisma:gen": "ts-node scripts/gen-prisma-clients.ts",
    "prisma:migrate": "ts-node scripts/db-migrate-all.ts",
    "db:test": "ts-node scripts/test-db-connection.ts",
    "env:validate": "ts-node scripts/validate-env.ts",
    "infra:up": "docker compose up -d",
    "infra:down": "docker compose down -v",
    "install:legacy": "npm install --legacy-peer-deps"
  },
